    <script>
    document.addEventListener("DOMContentLoaded", function (event) {
        stickyFooter.init();
        @*
            var n = new Noty({
                text: 'DONATE NOW!',
                type: 'success',
                theme: 'relax',
                layout: 'top',
                buttons: [
                    Noty.button('DONATE', 'btn btn-success', function () {
                        window.location = "/Donate";
                    }, { id: 'button1', 'data-status': 'ok' }),
                    Noty.button('No thanks', 'btn btn-error', function () {
                        n.close();
                    }, { id: 'button1', 'data-status': 'ok' })
                ]
            }).show();
        *@
    });
    </script>
    @RenderSection("Scripts")
    <script>
        $('.sign-in').magnificPopup({
            type: 'inline',
            preloader: false,
            focus: '#txtUserName',
            prependTo: 'form',
            closeOnBgClick: false,

            // When element is focused, some mobile browsers in some cases zoom in
            // It looks not nice, so we disable it:
            callbacks: {
                beforeOpen: function () {
                    if ($(window).width() < 700) {
                        this.st.focus = false;
                    } else {
                        this.st.focus = '#txtUserName';
                    }
                }
            }
        });
        $('.sign-up').magnificPopup({
            type: 'inline',
            preloader: false,
            focus: '#txtSignUpUser',
            prependTo: 'form',
            closeOnBgClick: false,
            callbacks: {
                beforeOpen: function () {
                    if ($(window).width() < 700) {
                        this.st.focus = false;
                    } else {
                        this.st.focus = '#txtSignUpUser';
                    }
                }
            }
        });
        $('.reset-password').magnificPopup({
            type: 'inline',
            preloader: false,
            focus: '#txtUserEmail',
            prependTo: 'form',
            closeOnBgClick: false,
            callbacks: {
                beforeOpen: function () {
                    if ($(window).width() < 700) {
                        this.st.focus = false;
                    } else {
                        this.st.focus = '#txtUserEmail';
                    }
                }
            }
        });
    </script>
