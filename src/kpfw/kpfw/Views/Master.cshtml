    height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) -->
    <div id="skip"><a href="#content">Skip to Main Content</a></div>
    <div class="toggle-menu lr">
        <ul>
            <li>
                <a id="menu-trigger" href="#0">
                    <span class="menu-text">Menu</span>
                    <div class="icon"></div>
                </a>
            </li>
            @*<li>
                <a id="account-trigger" href="#0"><span class="fa fa-user-circle"></span>Account</a>
            </li>*@
        </ul>
    </div>

    <nav aria-label="main mobile navigation" class="main-nav-copy lr" id="main-nav-copy"></nav>
    @*<nav aria-label="main mobile navigation" class="account-nav-copy lr" id="account-nav-copy"></nav>*@

    <div class="full homepage" data-sticky-wrap>
        <header class="main">
            <div class="top cf">
                <div class="left"></div>
                <nav aria-label="Primary Navigation" class="main-nav lr" id="main-nav">
                    <ul>
                        <li>@Html.RouteLink("Home", "default", new { controller = "Home", action = "Index" })</li>
                        <li><a href="/Guides">Guides</a></li>
                        <li>@Html.RouteLink("Caps", "default", new { controller = "Caps", action = "Index" })</li>
                        <li><a href="/Chat">Chat</a></li>
                        <li><a href="/About">About</a></li>
                        <li><a href="/Contact">Contact Us</a></li>
                        <li><a href="/Donate" class="donate-nav-btn">Donate</a></li>
                    </ul>
                </nav>
                @if (!User.Identity.IsAuthenticated)
                {
                    <nav aria-label="Account Navigation" class="account-nav lr" id="account-nav">
                        <ul>
                            <li>
                                <a class="btn sign-in" href="#loginModalPopup">Login</a>
                            </li>
                            <li>
                                <a class="btn sign-up" href="#signupModalPopup">Sign Up</a>
                            </li>
                        </ul>
                    </nav>
                }
                else
                {
                    <nav aria-label="Account Navigation" class="account-nav lr" id="account-nav">
                        <ul>
                            <li>
                                <asp:LinkButton runat="server" ID="lbLogout" OnClick="lbLogout_Click" CssClass="btn" Text="Logout" />
                            </li>
                            <li>
                                <a class="btn" href="#">My Account</a>
                            </li>
                        </ul>
                    </nav>
                }
            </div>
            <div class="logo">
                <a href="/">
                    <img src="//cdn.kpfanworld.com/images/logo.jpg" />
                </a>
            </div>
        </header>

        @RenderBody()
    </div>
    <!--end full-->
    <footer class="main" data-sticky-footer>
        <div class="inner">
            <p>© 2006-@DateTime.Today.Year <a href="/">Kim Possible Fan World</a>.</p>
